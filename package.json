{
  "name": "too-much-artisans-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "eslint": "eslint --ext .ts src",
    "prettier": "prettier -c \"./src/**/*.ts\"",
    "prettier:write": "prettier -c \"./src/**/*.ts\" --write",
    "lint": "yarn eslint && yarn prettier",
    "watch:serve": "nodemon",
    "serve": "ts-node -r dotenv/config src/server.ts",
    "watch:inspect": "nodemon --inspect -e yaml,ts,gql -w ./src -x node -r ts-node/register src/server.ts",
    "build": "rm -rf dist && npx tsc && cp -R src/assets/ dist/assets/"
  },
  "author": "Vincent LE GOFF",
  "license": "MIT",
  "dependencies": {
    "@promster/fastify": "5.0.0",
    "@promster/types": "^2.0.0",
    "axios": "0.21.0",
    "canvas": "2.6.1",
    "fastify": "3.9.1",
    "fastify-cors": "4.1.0",
    "fastify-gql": "5.8.2",
    "fastify-multipart": "3.3.1",
    "fastify-openapi-glue": "2.3.0",
    "fastify-swagger": "3.5.0",
    "js-yaml": "3.14.1",
    "lodash": "4.17.20",
    "lru_map": "0.4.1",
    "pino": "6.7.0",
    "pino-pretty": "4.3.0",
    "prom-client": "12.0.0",
    "tablemark": "2.0.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@ava/typescript": "1.1.1",
    "@types/lodash": "^4.14.165",
    "@types/node": "14.14.12",
    "@types/pino": "^6.3.4",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "ava": "3.14.0",
    "dotenv": "8.2.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "3.2.0",
    "gqlint": "1.8.0",
    "node-html-parser": "2.0.0",
    "nodemon": "2.0.6",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.1.2"
  },
  "imports": {
    "#app/*": {
      "require": "./*.js"
    }
  }
}
